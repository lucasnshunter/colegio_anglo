
<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">Arquivo</h3>
  </div>
  <div class="panel-body">
 <p id="notice"><%= notice %></p>

<p>
  <strong>Titulo:</strong>
  <%= @arquivo.titulo %>
</p>

<p>
  <strong>Descricao:</strong>
  <%= @arquivo.descricao %>
</p>

<p>
  <strong>Data criacao:</strong>
  <%= @arquivo.data_criacao %>
</p>

<p>
  <strong>Data prioridade:</strong>
  <%= @arquivo.data_prioridade %>
</p>
<hr>
  <%= link_to "Baixar", asset_path(@arquivo.arquivo.url),class:"btn btn-success" %>

  </div>
</div>
<%= link_to 'Editar', edit_arquivo_path(@arquivo),class:"btn btn-warning" %> |
<%= link_to 'voltar', arquivos_path , class:"btn btn-default" %>